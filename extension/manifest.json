{
  "name": "Example Gmail Extension",
  "description": "Example extension showing use of the Streak Gmail SDK",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "edit_console.html",
    "default_icon": "blundr_logo.png"
  },
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "https://prod3.dauntless.click",
    "activeTab"
  ],
  "manifest_version": 2
}
